{
  "name": "bully",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npx tsc -b && npx vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "make-parser": "peggy -o lib/booleanExpressionParser.ts --format es --dts src/parser/booleanExpression.peggy",
    "make-dev-parser": "peggy -o lib/booleanExpressionParser.ts --format es --dts --trace src/parser/booleanExpression.peggy"
  },
  "dependencies": {
    "peggy": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "less": "^4.3.0",
    "pegjs-backtrace": "^0.2.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vitest": "^3.1.1"
  }
